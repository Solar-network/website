{% extends 'layout.twig' %} {% block body %}

<body data-spy="scroll" data-target="#mobile-wallet-menu" data-offset="150">



    {% include "/blocks/header.twig" %}

    <section class="banner-7 bg_img oh bottom_right" data-background="./assets/images/banner/banner-bg-7.jpg">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <div class="section-header  coverage-header">

                        <h2 class="title">Staking</h2>
                        <p>
                            Earn SXP by becoming a delegate or by voting for delegates.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--============= Pricing Section Starts Here =============-->
    <section class="pricing-section padding-top-2 pt-lg-0 oh" style="margin-bottom:25px;" id="pricing">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-7 col-xl-6">
                    <div class="pricing-content">
                        <div class="section-header left-style mobile-center">
                            <h5 class="cate">*approximate placeholder</h5>
                            <h2 class="title">Staking Calculator </h2>
                            <p>
                                placeholder text about staking calculator
                            </p>
                        </div>
                        <div class="button-group mobile-center">
                            <a href="#0" class="button-3 active">Become Delegate</a>
                            <a href="#0" class="button-3 active">Become Voter</a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5 col-xl-6">
                    <div class="range-wrapper">
                        <div class="shape-1 d-none d-lg-block">
                            <img src="./assets/images/bg/1.png" alt="bg">
                        </div>
                        <div class="shape-2 d-none d-lg-block">
                            <img src="./assets/images/bg/2.png" alt="bg">
                        </div>
                        <div class="pricing-range">
                            <div class="pricing-range-top">
                                <div class="pricing-header">
                                    <span class="cate">Pricing Options</span>
                                    <div class="select-container">
                                        <select class="select-bar">
                                            <option value="basic">Voter</option>
                                            <option value="standard">Delegate</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="amount-area">
                                    <div class="item">
                                        <h2 class="title">15</h2>
                                        <span class="info">SXP Per Month</span>
                                    </div>
                                    <div class="item">
                                        <h2 class="title">199</h2>
                                        <span class="info">SXP Per Year</span>
                                    </div>
                                </div>
                                <div class="invest-range-area">
                                    <div class="invest-amount" data-min="1.00 USD" data-max="1000 USD">
                                        <div id="usd-range" class="invest-range-slider"></div>
                                    </div>
                                </div>
                            </div>
                            <div class="pricing-range-bottom">
                                <a href="#0" class="custom-button">Download App</a>
                                <ul class="download-options">
                                    <li>
                                        <a href="#0" class="active"><i class="fab fa-windows"></i></a>
                                    </li>
                                    <li>
                                        <a href="#0"><i class="fab fa-apple"></i></a>
                                    </li>
                                    <li>
                                        <a href="#0"><i class="fab fa-android"></i></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    </div>

    <!--============= Pricing Section Ends Here =============-->
    <section class="how-section padding-bottom padding-top pos-rel pb-xl-0 oh " id="how">
        <div class="bg-xl-100 bg_img"></div>
        <div class="top-shape d-none d-lg-block"><img src="./assets/css/img/work-shape.png" alt="css"></div>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="client-header">
                        <div class="section-header cl-white mb-olpo">
                            <h5 class="cate">palceholderp</h5>
                            <h2 class="header-title">placeholder</h2>
                            <p class="header-title">placeholder</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center mb-30-none">
                <div class="col-xl-6 col-sm-6">
                    <div class="how-item-3">
                        <div class="how-thumb">
                            <img src="./assets/images/how/how4.png" alt="how">
                        </div>
                        <div class="how-content">
                            <h4 class="title">Voter</h4>
                            <p>Text about voter placeholder
                            </p>
                            <a href="#0" class="button-3 active">Get Started <i class="flaticon-right"></i></a>
                        </div>
                    </div>
                </div>
                <div class="col-xl-6 col-sm-6">
                    <div class="how-item-3">
                        <div class="how-thumb">
                            <img src="./assets/images/how/how5.png" alt="how">
                        </div>
                        <div class="how-content">
                            <h4 class="title">Delegate</h4>
                            <p>text about delegate placeholder</p>
                            <a href="#0" class="button-3 active">Get Started <i class="flaticon-right"></i></a>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </section>
    <!--============= Comunity Section Starts Here =============-->
    <section class="comunity-section padding-top padding-bottom oh pos-rel">
        <div class="comunity-bg bg_img"></div>
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-7">
                    <div class="section-header cl-white">
                        <h5 class="cate">Still unsure?</h5>
                        <h2 class="title header-title">Ask the community</h2>

                    </div>
                </div>
            </div>
            <ul class="social-icons">
                <li>
                    <a href="#0"><i class="fab fa-facebook-f"></i></a>
                </li>
                <li>
                    <a href="#0" class="active"><i class="fab fa-twitter"></i></a>
                </li>
                <li>
                    <a href="#0"><i class="fab fa-pinterest-p"></i></a>
                </li>
                <li>
                    <a href="#0"><i class="fab fa-google-plus-g"></i></a>
                </li>
                <li>
                    <a href="#0"><i class="fab fa-instagram"></i></a>
                </li>
            </ul>
        </div>
    </section>

    {% include "/blocks/footer.twig" %}
</body>
{% endblock %}